{
  "name": "audio-dashcam",
  "version": "0.1.0",
  "description": "A React Native application that continuously records audio in the background using a circular buffer, allowing users to save the last 30 minutes of audio to Google Drive with a single tap.",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:emulator": "jest --testPathPattern=test/emulator",
    "test:e2e": "detox test -c android.emu.debug",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "cd android && ./gradlew clean && cd ..",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:android-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "pod-install": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-audio": "^4.3.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "googleapis": "^128.0.0",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-background-task": "^0.2.1",
    "@react-native-community/push-notification": "^8.1.1",
    "react-native-permissions": "^3.10.1",
    "react-native-fs": "^2.20.0",
    "react-native-device-info": "^10.11.0",
    "react-native-network-info": "^5.2.1",
    "react-native-vector-icons": "^10.0.2",
    "react-native-sound": "^0.11.2",
    "react-native-background-fetch": "^4.2.1",
    "react-native-background-job": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.4.2",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.10.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "keywords": [
    "react-native",
    "audio",
    "recording",
    "background",
    "circular-buffer",
    "google-drive",
    "dashcam",
    "android"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/audio-dashcam.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/audio-dashcam/issues"
  },
  "homepage": "https://github.com/yourusername/audio-dashcam#readme"
} 